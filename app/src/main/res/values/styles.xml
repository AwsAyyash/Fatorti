<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="AppTheme.DayNight" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.ForceDark" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:forceDarkAllowed" tools:targetApi="q">true</item>
    </style>
    <style name="mainScrnBtnStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">0.10</item>
        <item name="android:drawableLeft">@drawable/contact</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:layout_margin">10dp</item>
    </style>
</resources>